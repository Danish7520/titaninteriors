   <!-- typewriting -->
   <div style="margin-top:40px">
      <center>
          <h1>
              <a style="font-weight:700; color:#A10029; " href="" class="typewrite" data-period="2000" data-type='[ "Welcome to titan interiors.", "Get free of cost Estimates.", "We offer various interior items." ]'>
    <span class="wrap"></span>
